us = "YOUR ADDRESS HERE"
evm_code = "".decode("hex")
creation_tx = eth.transact(to='', data=evm_code, startgas=2000000, sender=us)
abi = """[{"constant":false,"inputs":[{"name":"VK","type":"string"},{"name":"messageHash","type":"string"},{"name":"signature","type":"string"}],"name":"thresholdVerify","outputs":[{"name":"","type":"bool"}],"type":"function"},{"anonymous":false,"inputs":[],"name":"SubroutineFailed","type":"event"},{"anonymous":false,"inputs":[{"indexed":false,"name":"","type":"bytes"}],"name":"SignatureSucceeded","type":"event"}]"""
signature_contract = eth.new_contract(abi, creation_tx.creates)
signature_contract.thresholdVerify.sender = us.decode("hex")
import time
time.sleep(0)
